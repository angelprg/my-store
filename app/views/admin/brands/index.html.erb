<div class="container-fluid">
  <div class="row">
    <%= render 'admin/sidebar' %>
    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      <%= render partial: 'admin/header' , locals: {title: 'Brands'}%>
      <%= link_to "New brand", new_admin_brand_path, class: "btn btn-primary" %>
      <div id="brands">
        <% @brands.each do |brand| %>
           <%= render partial: "brand_card", locals: {brand: brand} %>
        <% end %>
      </div>
    </main>
  </div>
</div>
